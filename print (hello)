# indentation 
sum =10
def calculate ():
    global sum
    sum  = 20
    currentsum =200
    totalsum = sum + currentsum
    print (totalsum)
calculate()
print(sum)
print(sum)

sum = 30
def calculate ():
    sum = 20
    currentsum = 100
    totalsum = sum + currentsum
    print(totalsum)
calculate()
print (sum)

